{
	"name": "@weareinreach/i18next-keys-ondemand",
	"version": "0.2.2",
	"description": "i18next plugin to request the non-translated keys when used",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"type": "module",
	"repository": "https://github.com/weareinreach/i18next-keys-ondemand.git",
	"author": "Nabil Koroghli",
	"license": "MIT",
	"scripts": {
		"prepublish": "pnpm build",
		"test": "jest",
		"build": "ncc build ./src/index.ts -o ./dist --target es2020"
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@types/debounce": "^1.2.1",
		"@types/gulp": "^4.0.10",
		"@types/jest": "^29.4.0",
		"@types/merge2": "^1.4.0",
		"@vercel/ncc": "^0.36.1",
		"gulp": "^4.0.2",
		"gulp-clean": "^0.4.0",
		"gulp-tslint": "^8.1.4",
		"gulp-typescript": "6.0.0-alpha.1",
		"i18next": "^22.4.10",
		"jest": "^29.4.3",
		"merge2": "^1.4.1",
		"ts-jest": "^29.0.5",
		"tslint": "^6.1.3",
		"typescript": "^4.9.5",
		"whatwg-fetch": "^3.6.2"
	},
	"dependencies": {
		"debounce": "^1.2.1"
	},
	"peerDependencies": {
		"i18next": "22.x.x"
	},
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"setupFiles": [
			"<rootDir>/config/polyfills.js"
		],
		"transform": {
			"^.+\\.tsx?$": "<rootDir>/config/typescriptTransform.js"
		},
		"testMatch": [
			"**/?(*.)(spec|test).ts?(x)"
		]
	}
}
